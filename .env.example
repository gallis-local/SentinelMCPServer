# Example .env file for SentinelMCPServer

# Sentinel configuration
SENTINEL_SUBSCRIPTION_ID=your_subscription_id_here
SENTINEL_RESOURCE_GROUP=your_resource_group_here
SENTINEL_WORKSPACE_NAME=your_workspace_name_here
SENTINEL_WORKSPACE_ID=your_workspace_id_here

# Azure authentication (required for client_secret authentication)
AZURE_TENANT_ID=your_tenant_id_here
AZURE_CLIENT_ID=your_client_id_here
AZURE_CLIENT_SECRET=your_client_secret_here